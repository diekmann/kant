session Kant = HOL +
  (*pass document=pdf via iabelle build command line *)
  options [document_output = "output"]
  theories [document = false]
    ExecutableHelper
  theories
    Gesetz
    Kant
    BeispielZahlenwelt
    BeispielSteuern
  document_files
    "root.tex"


session Slides in slides = Kant +
  (*pass document=pdf via iabelle build command line *)
  options [document_output = "output", document_echo, document_logo = "_"]
  theories
    Slides
  document_files
    "root.tex"